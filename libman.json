{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "provider": "cdnjs",
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation@1.20.0",
      "destination": "wwwroot/lib/jquery-validation/"
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "admin-lte@3.2.0",
      "destination": "wwwroot/plugins/admin-lte/",
      "files": [
        "js/adminlte.min.js",
        "css/adminlte.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "datatables@1.10.21",
      "destination": "wwwroot/plugins/datatables-bs4/",
      "files": [
        "js/jquery.dataTables.min.js",
        "css/dataTables.bootstrap4.css",
        "js/dataTables.bootstrap4.js",
        "css/dataTables.bootstrap4.min.css",
        "js/dataTables.bootstrap4.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "datatables-responsive@1.0.7",
      "destination": "wwwroot/plugins/datatables-responsive/",
      "files": [
        "js/dataTables.responsive.js",
        "js/dataTables.responsive.min.js",
        "js/responsive.bootstrap.js",
        "js/responsive.bootstrap.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "fontawesome-free@1.0.4",
      "destination": "wwwroot/plugins/fontawesome-free/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "css/brands.css",
        "css/brands.min.css",
        "css/fontawesome.css",
        "css/fontawesome.min.css",
        "css/regular.css",
        "css/regular.min.css",
        "css/solid.css",
        "css/solid.min.css",
        "css/svg-with-js.css",
        "css/svg-with-js.min.css",
        "css/v4-shims.css",
        "css/v4-shims.min.css",
        "webfonts/fa-brands-400.eot",
        "webfonts/fa-brands-400.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.eot",
        "webfonts/fa-regular-400.svg",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.eot",
        "webfonts/fa-solid-900.svg",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "icheck-bootstrap@3.0.1",
      "destination": "wwwroot/plugins/icheck-bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "sweetalert2@11.10.2",
      "destination": "wwwroot/plugins/sweetalert2/"
    }
  ]
}